// Menggunakan List (Array)
let listMahasiswa = ["Bars", "Puteri", "Mugar"];
listMahasiswa.push("Didi"); // Menambahkan elemen baru
console.log(listMahasiswa); // Output: ["Bars", "Puteri", "Mugar", "Didi"]

// Menggunakan Set
let setMahasiswa = new Set(["Bars", "Puteri", "Mugar"]);
setMahasiswa.add("Didi");
setMahasiswa.add("Bars"); // Tidak akan menambahkan karena Dimas sudah ada
console.log(setMahasiswa); // Output: Set(4) {"Bars", "Puteri", "Mugar", "Didi"}

// Menggunakan Map
let mapNilai = new Map();
mapNilai.set("Bars", 90);
mapNilai.set("Puteri", 85);
console.log(mapNilai.get("Bars")); // Output: 90
